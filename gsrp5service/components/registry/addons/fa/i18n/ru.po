#: model@fa.accounts|_columns@opening_debit$label:1
msgid "Opening debit"
msgstr "Открытие дебетовой"

#: model@fa.accounts|_columns@opening_debit$help:1
msgid "Opening debit value for this account."
msgstr "Открытие дебетовой значение для этой учетной записи."

#: model@fa.accounts|_columns@opening_credit$label:1
msgid "Opening credit"
msgstr "Открытие кредитной"

#: model@fa.accounts|_columns@opening_credit$help:1
msgid "Opening credit value for this account."
msgstr "Открытие стоимость кредита для этой учетной записи."

#: model@fa.accounts.group|_description@:1
msgid "Genaral Model Accounts Group"
msgstr "Общая Модель Счетах Группы"

#: model@fa.accounts.group|_columns@name$label:1
#: model@fa.account.template|_columns@name$label:1
#: model@fa.chart.template|_columns@name$label:1
