msgid "It acts as a default account for credit amount"
msgstr "Ajo vepron si një default llogari për kredi shuma"

#: model@fa.accounts.journal|_columns@default_debit_account_id$label:1
msgid "Default Debit Account"
msgstr "Default Debitore E Llogarisë"

