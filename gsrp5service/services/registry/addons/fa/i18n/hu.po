msgstr "Azért rendeltem, Folyóiratok a műszerfal megtekintése"

#: model@fa.accounts.journal|_columns@sequence_number_next$label:1
msgid "Next Number"
msgstr "Következő Szám"

#: model@fa.accounts.journal|_columns@sequence_number_next$help:1
msgid "The next sequence number will be used for the next invoice."
msgstr "A következő sorszám fogja használni a következő számla."

#: model@fa.accounts.journal|_columns@refund_sequence_number_next$label:1
msgid "Credit Notes: Next Number"
msgstr "Hitel Megjegyzés: A Következő Szám"

#: model@fa.accounts.journal|_columns@refund_sequence_number_next$help:1
msgid "The next sequence number will be used for the next credit note."
msgstr "A következő sorszám fogja használni a következő hitel megjegyzés."

#: model@fa.accounts.journal|_columns@currency_id$label:1
#: model@fa.chart.template|_columns@currency_id$label:1
msgid "Currency"
msgstr "Pénznem"

